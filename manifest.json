{
    "manifest_version": 3,
    "name": "Scratch Paper",
    "version": "1.0",
    "description": "A simple popup for taking notes and organizing them easily.",
    "permissions": [
      "storage",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html"
  }
  